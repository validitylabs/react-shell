    "dependencies": {
        "@material-ui/core": "^4.9.12",
        "@material-ui/icons": "^4.9.1",
        "axios": "^0.19.2",
        "pug": "^2.0.4",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-helmet": "^6.0.0",
        "react-swipeable": "^5.5.1"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.4",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/react-helmet": "^5.0.15",
        "@types/shelljs": "^0.8.7",
