        "react-dom": "^16.13.0"
    },
    "devDependencies": {
        "@types/node": "^13.13.4",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "cross-env": "^7.0.2",
        "parcel-bundler": "^1.12.4",
        "typescript": "^3.8.3"
